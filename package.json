{
  "name": "littil-org-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "openapi": "openapi-generator-cli generate -i ./openapi.json -g typescript-angular -o src/app/api/generated --skip-validate-spec --type-mappings=set=Array",
    "postinstall": "npm run openapi"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.2.0",
    "@angular/cdk": "14.2.0",
    "@angular/common": "14.2.0",
    "@angular/compiler": "14.2.0",
    "@angular/core": "14.2.0",
    "@angular/forms": "14.2.0",
    "@angular/material": "14.2.0",
    "@angular/platform-browser": "14.2.0",
    "@angular/platform-browser-dynamic": "14.2.0",
    "@angular/router": "14.2.0",
    "@asymmetrik/ngx-leaflet": "^14.0.1",
    "@auth0/auth0-angular": "1.10.1",
    "@types/google.maps": "3.52.1",
    "feedbackfin": "^1.1.0",
    "cookieconsent": "^3.1.1",
    "jest-junit": "14.0.1",
    "leaflet": "^1.9.4",
    "ngx-cookieconsent": "^4.0.2",
    "rxjs": "7.8.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.10",
    "@angular/cli": "14.2.0",
    "@angular/compiler-cli": "14.2.0",
    "@googlemaps/jest-mocks": "2.7.2",
    "@ngneat/spectator": "11.1.3",
    "@openapitools/openapi-generator-cli": "2.5.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/events": "^3.0.0",
    "@types/jest": "29.0.0",
    "@types/leaflet": "^1.9.3",
    "@types/node": "18.7.15",
    "autoprefixer": "10.4.8",
    "jest": "28.0.0",
    "jest-preset-angular": "12.2.2",
    "msw": "^1.2.1",
    "ng-mocks": "14.2.0",
    "postcss": "8.4.16",
    "tailwindcss": "3.1.8",
    "tslib": "^2.5.0",
    "typescript": "4.8.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/infrastructure/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/app/api/generated"
    ],
    "reporters": [
      "default"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov",
      "cobertura"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 45,
        "functions": 45,
        "lines": 70,
        "statements": 70
      }
    }
  }
}
